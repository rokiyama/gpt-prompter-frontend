  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "~18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.4"
  },
  "private": true
